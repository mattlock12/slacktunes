events {}

http {
  error_log /etc/nginx/error_log.log warn;
  
  server {
    listen 80 default_server;
    server_name slacktunes.me www.slacktunes.me;
    return 301 https://slacktunes.me$request_uri;
  }

  server {
    listen 443 ssl default_server;
    server_name slacktunes.me www.slacktunes.me;  
  }
}